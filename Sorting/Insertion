#include <stdio.h>

void insertionSort(int a[],int n);
void printArray(int a[],int n);

int main()
{
    int i,n;
    printf("Enter the no of elements of array: \n");
    scanf("%d",&n);
    
    int a[n];
    printf("Enter the elements of array: \n");
    for(i=0;i<n;i++){
    scanf("%d",&a[i]);
    }
    
    insertionSort(a,n);
    printArray(a,n);
    return 0;
}

void insertionSort(int a[],int n)
{
    int i,j,temp;
    for( i=1;i<n;i++)
    {
        int temp=a[i];
        j=i-1;
        while( j>=0 && a[j]>temp)
        {
            a[j+1]=a[j];
            j--;
        }
        a[j+1]=temp;
    }
}
void printArray(int a[],int n)
{
    int i;
    for(i=0;i<n;i++)
    {
        printf("%d\n",a[i]);
    }
}
